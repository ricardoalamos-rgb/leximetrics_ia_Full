{
    "name": "@leximetrics/db",
    "version": "0.0.1",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "scripts": {
        "build": "prisma generate && tsc",
        "build:seed": "tsc prisma/seed.ts --outDir dist/prisma --skipLibCheck --module commonjs --target es2019 --esModuleInterop && cp prisma/causas_seed.json dist/prisma/causas_seed.json",
        "generate": "prisma generate",
        "push": "prisma db push",
        "studio": "prisma studio"
    },
    "dependencies": {
        "@prisma/client": "5.22.0",
        "bcryptjs": "^3.0.3",
        "prisma": "5.22.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^3.0.0",
        "@types/node": "^20.11.24",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.3"
    },
    "prisma": {
        "seed": "node dist/prisma/seed.js"
    }
}